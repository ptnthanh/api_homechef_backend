{
  "name": "homechef_backend",
  "version": "1.0.0",
  "description": "Backend for user database management for homechef app",
  "main": "server.js",
  "scripts": {
    "test": "mocha --require babel-core/register ./test/**/*.js --timeout 10000",
    "create-dev-tables": "babel-node ./app/db/dev/dbConnection createAllTables",
    "seed-user-table": "babel-node ./app/helpers/seed seedUser",
    "start": "nodemon --watch . --exec babel-node -- server",
    "setup": "npm-run-all -p start create-dev-tables"
  },
  "engines": {
    "node": "v12.16.3",
    "npm": "6.14.5"
  },
  "author": "Quan Phan",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "babel": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "braces": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0"
  }
}
